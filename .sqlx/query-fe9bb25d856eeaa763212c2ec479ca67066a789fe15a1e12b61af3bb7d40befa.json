  "query": "\n        SELECT\n            id,\n            name,\n            description,\n            project_id,\n            created_at,\n            updated_at\n        FROM role\n        WHERE ($1 OR project_id = $2)\n            AND ($3 OR id = any($4))\n            AND ($5 OR name ILIKE ('%' || $6 || '%'))\n        ",
